{
  "name": "mimic-azure-openai",
  "version": "1.0.0",
  "description": "A service that mimics Azure OpenAI chat completions endpoint with streaming",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "build:http": "npx tsc src/server.ts --outDir dist",
    "build:https": "npx tsc src/server-https.ts --outDir dist",
    "build:docker": "npx tsc src/server-docker.ts --outDir dist",
    "start": "node dist/server.js",
    "start:http": "npm run build:http && node dist/server.js",
    "start:https": "npm run build:https && node dist/server-https.js",
    "start:docker": "npm run build:docker && node dist/server-docker.js",
    "start:proxy": "npm run build:https && set USE_EDUHK_PROXY=true && node dist/server-https.js",
    "start:mock": "npm run build:https && set USE_EDUHK_PROXY=false && node dist/server-https.js",
    "dev": "npm run build:http && node dist/server.js",
    "dev:https": "npm run build:https && node dist/server-https.js",
    "dev:docker": "npm run build:docker && node dist/server-docker.js",
    "dev:proxy": "npm run build:https && set USE_EDUHK_PROXY=true && node dist/server-https.js",
    "dev:mock": "npm run build:https && set USE_EDUHK_PROXY=false && node dist/server-https.js",
    "generate-certs": "node generate-certs.js",
    "docker:build": "docker build -t azure-openai-mock .",
    "docker:run": "docker run -p 5555:5555 -p 5556:5556 --env-file .env azure-openai-mock",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart"
  },
  "keywords": [
    "azure",
    "openai",
    "mimic",
    "streaming"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^17.2.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
