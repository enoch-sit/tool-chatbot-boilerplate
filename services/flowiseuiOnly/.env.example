# Example environment file - copy to .env and configure
VITE_FLOWISE_BASE_URL=https://your-flowise-instance.com
VITE_FLOWISE_CHATFLOW_ID=your-chatflow-id-here

# FlowiseAI Authentication
# Use API Key method (Bearer token) - RECOMMENDED for FlowiseAI
VITE_FLOWISE_API_KEY=your-flowise-api-key

# Application Configuration
VITE_BASE_PATH=/your-base-path
VITE_PORT=3002

# Optional settings
VITE_APP_TITLE=FlowiseAI Chat
VITE_STREAMING_ENABLED=true